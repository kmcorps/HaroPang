<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.haroid.HaroPang"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="6"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application android:label="@string/app_name"
                 android:icon="@drawable/icon" >
        <activity android:name=".HaroPang"
                  android:label="@string/app_name"
                  android:configChanges="orientation|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DeviceListActivity"
                  android:label="@string/select_device"
                  android:theme="@android:style/Theme.Dialog"
                  android:configChanges="orientation|keyboardHidden" />
        <activity android:name="HaroPangStage0"></activity>
        <activity android:name="HaroPangStage1"></activity>
        <activity android:name="HaroPangStage2"></activity>
        <activity android:name="HaroPangStage3"></activity>
        <activity android:name="HaroPangStage4"></activity>
        <activity android:name="HaroPangStage5"></activity>
        <activity android:name="HaroPangStage6"></activity>
        <activity android:name="HaroPangStage7"></activity>
        <activity android:name="HaroPangStage8"></activity>
        <activity android:name="HaroPangStage9"></activity>
    </application>
</manifest>